{
  "name": "@uncharted/lex",
  "description": "A preact framework for building token-based search bars",
  "version": "0.0.1",
  "private": true,
  "author": {
    "name": "Sean McIntyre",
    "email": "smcintyre@uncharted.software"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/unchartedsoftware/lex.git"
  },
  "scripts": {
    "start": "npm run serve",
    "clean": "rimraf dist/",
    "build": "npm run clean && npm run lint && npm run parcel-js && npm run parcel-css",
    "parcel-js": "parcel build src/lex.js --no-minify",
    "parcel-css": "parcel build src/style/index.scss --no-minify",
    "serve": "NODE_ENV=development parcel test/index.html",
    "lint": "eslint ./src",
    "prepublishOnly": "npm run build",
    "docs": "documentation build src/lex.js -f html -o docs",
    "serve-docs": "documentation serve --watch src/lex.js"
  },
  "main": "dist/lex.js",
  "devDependencies": {
    "bootstrap": "3.3.7",
    "babel-eslint": "8.0.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-preset-env": "1.6.1",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-preact-require": "1.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-react-jsx": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "decko": "1.2.0",
    "documentation": "5.3.3",
    "eslint": "4.10.0",
    "eslint-config-semistandard-react": "4.2.0",
    "eslint-plugin-semistandard-react": "4.1.0",
    "eslint-config-semistandard": "11.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-react": "7.4.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-node": "5.2.1",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-promise": "3.6.0",
    "eslint-plugin-jsdoc": "3.1.3",
    "if-env": "1.0.0",
    "node-sass": "4.6.0",
    "preact": "8.x",
    "linkstate": "1.1.x",
    "sass-loader": "6.0.6",
    "wolfy87-eventemitter": "5.2.4",
    "preact-portal": "1.1.x",
    "parcel-bundler": "1.1.0",
    "rimraf": "2.6.2"
  }
}
